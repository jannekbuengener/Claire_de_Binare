{
  "dashboard": {
    "title": "Claire de Binare - Paper Trading",
    "tags": ["claire", "paper-trading"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Signals Generated (Total)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "signals_generated_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Signal Engine Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "signal_engine_status",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "options": {"0": {"text": "STOPPED", "color": "red"}}},
              {"type": "value", "options": {"1": {"text": "RUNNING", "color": "green"}}}
            ]
          }
        }
      },
      {
        "id": 3,
        "title": "Orders Approved",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "orders_approved_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Orders Blocked",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "orders_blocked_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 20},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Approval Rate (%)",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "(orders_approved_total / (orders_approved_total + orders_blocked_total)) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "green", "value": 20}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 4},
        "targets": [
          {
            "expr": "circuit_breaker_active",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "options": {"0": {"text": "INACTIVE", "color": "green"}}},
              {"type": "value", "options": {"1": {"text": "ACTIVE", "color": "red"}}}
            ]
          }
        }
      },
      {
        "id": 7,
        "title": "Order Results Received",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 4},
        "targets": [
          {
            "expr": "order_results_received_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 8,
        "title": "Signals Generated Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 10},
        "targets": [
          {
            "expr": "rate(signals_generated_total[5m])",
            "refId": "A",
            "legendFormat": "Signals/sec"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Orders Approved vs Blocked",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 18},
        "targets": [
          {
            "expr": "rate(orders_approved_total[5m])",
            "refId": "A",
            "legendFormat": "Approved/sec"
          },
          {
            "expr": "rate(orders_blocked_total[5m])",
            "refId": "B",
            "legendFormat": "Blocked/sec"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      }
    ]
  }
}
