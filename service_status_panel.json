{
  "datasource": {
    "type": "prometheus",
    "uid": "df53l13ov7gu8c"
  },
  "description": "Service Health & Circuit Breaker Status",
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "thresholds"
      },
      "custom": {
        "align": "auto",
        "cellOptions": {
          "type": "auto"
        },
        "inspect": false
      },
      "mappings": [
        {
          "options": {
            "0": {
              "color": "green",
              "index": 0,
              "text": "‚úÖ OK"
            },
            "1": {
              "color": "red",
              "index": 1,
              "text": "üî¥ ACTIVE"
            }
          },
          "type": "value"
        }
      ],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "red",
            "value": 1
          }
        ]
      }
    },
    "overrides": [
      {
        "matcher": {
          "id": "byName",
          "options": "Service"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 200
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Type"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 120
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Status"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 100
          },
          {
            "id": "mappings",
            "value": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "index": 1,
                    "text": "‚ùå DOWN"
                  },
                  "1": {
                    "color": "green",
                    "index": 0,
                    "text": "‚úÖ UP"
                  }
                },
                "type": "value"
              }
            ]
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Circuit Breaker"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 150
          }
        ]
      }
    ]
  },
  "gridPos": {
    "h": 8,
    "w": 12,
    "x": 0,
    "y": 10
  },
  "id": 202,
  "options": {
    "cellHeight": "sm",
    "footer": {
      "countRows": false,
      "fields": "",
      "reducer": ["sum"],
      "show": false
    },
    "showHeader": true,
    "sortBy": [
      {
        "desc": false,
        "displayName": "Type"
      }
    ]
  },
  "pluginVersion": "12.2.1",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "df53l13ov7gu8c"
      },
      "expr": "up{job=~\"signal_engine|risk_manager|execution_service|adaptive_intensity\"}",
      "format": "table",
      "instant": true,
      "legendFormat": "{{job}}",
      "refId": "Status"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "df53l13ov7gu8c"
      },
      "expr": "circuit_breaker_active or on(job) up{job=\"risk_manager\"} * 0",
      "format": "table",
      "hide": false,
      "instant": true,
      "refId": "CircuitBreaker"
    }
  ],
  "title": "üîß Service Status & Circuit Breaker",
  "transformations": [
    {
      "id": "merge",
      "options": {}
    },
    {
      "id": "organize",
      "options": {
        "excludeByName": {
          "Time": true,
          "__name__": true,
          "instance": true
        },
        "indexByName": {
          "Time": 0,
          "Value #CircuitBreaker": 5,
          "Value #Status": 4,
          "__name__": 1,
          "instance": 2,
          "job": 3,
          "service": 6
        },
        "renameByName": {
          "Value #CircuitBreaker": "Circuit Breaker",
          "Value #Status": "Status",
          "job": "Service",
          "service": "Type"
        }
      }
    }
  ],
  "type": "table"
}
