{
  "uid": "claire_signal_engine_v1",
  "title": "Claire - Signal Engine",
  "description": "Real-time signal generation and analysis dashboard",
  "tags": ["claire", "signals", "engine", "dark"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m"]
  },
  "style": "dark",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "SIGNALS GENERATED",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT COUNT(*) FROM signals WHERE timestamp > EXTRACT(EPOCH FROM NOW() - INTERVAL '1 hour') * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#1a1a1a", "value": null},
              {"color": "#2563eb", "value": 10},
              {"color": "#3b82f6", "value": 50},
              {"color": "#60a5fa", "value": 100}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "BUY SIGNALS",
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT COUNT(*) FROM signals WHERE signal_type = 'buy' AND timestamp > EXTRACT(EPOCH FROM NOW() - INTERVAL '1 hour') * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#1a1a1a", "value": null},
              {"color": "#22c55e", "value": 1}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "SELL SIGNALS",
      "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT COUNT(*) FROM signals WHERE signal_type = 'sell' AND timestamp > EXTRACT(EPOCH FROM NOW() - INTERVAL '1 hour') * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#1a1a1a", "value": null},
              {"color": "#ef4444", "value": 1}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "AVG CONFIDENCE",
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT ROUND(AVG(confidence)::numeric, 2) FROM signals WHERE timestamp > EXTRACT(EPOCH FROM NOW() - INTERVAL '1 hour') * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#ef4444", "value": null},
              {"color": "#f97316", "value": 0.3},
              {"color": "#16a34a", "value": 0.6},
              {"color": "#22c55e", "value": 0.8}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "value"
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "SIGNAL GENERATION RATE",
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 6},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT timestamp AS time, COUNT(*) as value FROM signals WHERE timestamp > EXTRACT(EPOCH FROM NOW() - INTERVAL '1 hour') * 1000 GROUP BY time ORDER BY time",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "bars",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 80,
            "gradientMode": "opacity",
            "spanNulls": false,
            "showPoints": "never"
          },
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "transparent", "value": null}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "value"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "#3b82f6"}}
            ]
          }
        ]
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {
          "displayMode": "hidden"
        }
      }
    },
    {
      "id": 6,
      "type": "bargauge",
      "title": "TOP SYMBOLS (Last Hour)",
      "gridPos": {"h": 10, "w": 12, "x": 0, "y": 16},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT symbol, COUNT(*) as count FROM signals WHERE timestamp > EXTRACT(EPOCH FROM NOW() - INTERVAL '1 hour') * 1000 GROUP BY symbol ORDER BY count DESC LIMIT 10",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#1a1a1a", "value": null},
              {"color": "#2563eb", "value": 5},
              {"color": "#3b82f6", "value": 10},
              {"color": "#60a5fa", "value": 20}
            ]
          }
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        },
        "showUnfilled": true
      }
    },
    {
      "id": 7,
      "type": "piechart",
      "title": "SIGNAL TYPE DISTRIBUTION",
      "gridPos": {"h": 10, "w": 12, "x": 12, "y": 16},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT signal_type, COUNT(*) as count FROM signals WHERE timestamp > EXTRACT(EPOCH FROM NOW() - INTERVAL '1 hour') * 1000 GROUP BY signal_type",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"}
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "buy"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "#22c55e"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "sell"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "#ef4444"}}
            ]
          }
        ]
      },
      "options": {
        "pieType": "donut",
        "displayLabels": ["name", "percent"],
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["sum"]
        }
      }
    },
    {
      "id": 8,
      "type": "table",
      "title": "RECENT SIGNALS (Live Feed)",
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 26},
      "datasource": "PostgreSQL",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT TO_TIMESTAMP(timestamp/1000) as time, symbol, signal_type, ROUND(confidence::numeric, 2) as confidence, ROUND(price::numeric, 2) as price, ROUND(pct_change::numeric, 2) as pct_change, reason FROM signals ORDER BY timestamp DESC LIMIT 50",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "signal_type"},
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "buy": {"color": "#22c55e", "index": 0},
                      "sell": {"color": "#ef4444", "index": 1}
                    }
                  }
                ]
              }
            ]
          },
          {
            "matcher": {"id": "byName", "options": "confidence"},
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {"color": "#ef4444", "value": null},
                    {"color": "#f97316", "value": 0.3},
                    {"color": "#16a34a", "value": 0.6},
                    {"color": "#22c55e", "value": 0.8}
                  ]
                }
              },
              {"id": "max", "value": 1},
              {"id": "min", "value": 0}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "pct_change"},
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {"color": "#1a1a1a", "value": null},
                    {"color": "#3b82f6", "value": 1},
                    {"color": "#60a5fa", "value": 3},
                    {"color": "#93c5fd", "value": 5}
                  ]
                }
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "footer": {
          "show": false
        }
      }
    }
  ]
}
